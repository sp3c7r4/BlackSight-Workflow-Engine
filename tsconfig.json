{
  "compilerOptions": {
    "target": "es2022",
    "module": "CommonJS",
    "strict": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "exactOptionalPropertyTypes": false,
    "outDir": "dist",
    "rootDir": "src",
    "esModuleInterop": true
  },
  "exclude": [
    "eslint.config.mts",
    "tsconfig.json",
    "test.ts"
  ]
}